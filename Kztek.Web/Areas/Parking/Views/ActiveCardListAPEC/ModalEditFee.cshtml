@using Kztek.Web.Core.Functions
@model Kztek.Model.Models.tblActiveCard
@{
    //var Dictionary = FunctionHelper.GetLocalizeDictionary("tblCard", "Index");
    //var DictionaryAction = FunctionHelper.GetLocalizeDictionary("Home", "Action");
}

<style>
    .loading1 {
        display: none;
    }
</style>

<div id="modalEditFee" class="modal fade">
    <div class="modal-dialog modal-md">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                <h4 class="modal-title" id="boxTitle">Chỉnh sửa tiền gia hạn</h4>
            </div>
            <form role="form" id="editFee" class="form-horizontal">
                <div class="modal-body" style="">
                    @Html.HiddenFor(n => n.Id)
                    <div class="form-group" style="margin-top:15px">
                        <label class="col-sm-2 control-label-left no-padding-right" style="padding-top : 8px ; left: 15px">Số tiền :</label>
                        <div class="col-sm-6 col-xs-12">
                            @Html.TextBoxFor(n => n.FeeLevel, new { @class = "form-control " , @type= "number" ,@min ="0" , @max ="9"})
                            @Html.ValidationMessageFor(n => n.FeeLevel, "", new { @class = "text-danger" })
                        </div>
                    </div>
                </div>
            </form>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
                <button type="button" class="btn btn-primary" id="btnSave" onclick="UpdateFee()">Lưu</button>
            </div>

        </div>
    </div>
</div>




